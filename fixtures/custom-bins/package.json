{
	"name": "@pakk/fixture-custom-bins",
	"description": "custom bins example",
	"version": "0.13.1",
	"license": "MIT",
	"private": true,
	"archetype": {
		"platform": "node",
		"language": "ts",
		"kind": "fixture"
	},
	"keywords": [
		"javascript",
		"js",
		"managed-by-autotool",
		"ts",
		"typescript",
		"vite",
		"vite-plugin"
	],
	"homepage": "https://github.com/AlexAegis/pakk-fake",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"lint:depcheck": "turbo run lint:depcheck_ --concurrency 16 --filter @pakk/fixture-custom-bins",
		"lint:depcheck_": "depcheck",
		"lint:es": "turbo run lint:es_ --concurrency 16 --filter @pakk/fixture-custom-bins",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 16 --filter @pakk/fixture-custom-bins",
		"lint:format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check .",
		"lint:md": "turbo run lint:md_ --concurrency 16 --filter @pakk/fixture-custom-bins",
		"lint:md_": "remark --frail --no-stdout --silently-ignore .",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 16 --filter @pakk/fixture-custom-bins",
		"lint:tsc_": "tsc --noEmit",
		"test": "turbo run test_ --concurrency 16 --cache-dir .cache/turbo --filter @pakk/fixture-custom-bins",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage",
		"all": "BUILD_REASON='publish' turbo run all_ --concurrency 16 --filter @pakk/fixture-custom-bins",
		"build": "turbo run build-lib_ --concurrency 16 --cache-dir .cache/turbo --filter @pakk/fixture-custom-bins",
		"build-lib_": "vite build",
		"format": "turbo run format_ --concurrency 16 --filter @pakk/fixture-custom-bins",
		"format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
		"pakk": "pakk --srcDir source --binBaseDir cli --bins '**/*' --binIgnore ignore --staticExports static --dts --enabledFeatues export --enabledFeatues bin --enabledFeatues copy-license --enabledFeatues export-static ",
		"postinstall": "# local install hooks are disabled # autogenerated"
	},
	"exports": {
		".": {
			"types": "./source/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"default": "./dist/index.js"
		},
		"./custom.config": {
			"types": "./source/custom.config.ts",
			"import": "./dist/custom.config.js",
			"require": "./dist/custom.config.cjs",
			"default": "./dist/custom.config.js"
		},
		"./package.json": "./package.json",
		"./some": {
			"types": "./source/some.d.ts",
			"import": "./dist/some.js",
			"require": "./dist/some.cjs",
			"default": "./dist/some.js"
		},
		"./test": "./static/test.txt"
	},
	"bin": {
		"bar": "./shims/inner/bar.js",
		"fixture-custom-bins": "./shims/index.js",
		"foo.config": "./shims/foo.config.js",
		"manual": "./manual.js",
		"pakk-fixture-custom-bins-postinstall": "./shims/postinstall.js"
	},
	"devDependencies": {
		"@alexaegis/eslint-config-vitest": "workspace:^",
		"@alexaegis/ts": "workspace:^0.14.0",
		"@types/node": "^22.10.1",
		"@vitest/coverage-v8": "^3.1.1",
		"vite": "^6.0.1",
		"vite-plugin-pakk": "workspace:^",
		"vitest": "^3.1.1"
	}
}
