{
	"$schema": "https://turborepo.org/schema.json",

	"pipeline": {
		"prebuild_": {
			"outputs": ["dist/**"],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#prebuild_": {
			"outputs": ["dist/**"],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"build_": {
			"env": ["BASE_HREF"],
			"dependsOn": ["^prebuild_", "^build_"],
			"outputs": ["dist/**"],
			"outputMode": "new-only"
		},
		"//#build_": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint_": {
			"outputs": [],
			"dependsOn": ["lint:es_", "lint:svelte_", "lint:style_", "lint:md_", "lint:format_"],
			"outputMode": "new-only"
		},
		"//#lint_": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:es_:chain": {
			"cache": false,
			"outputMode": "new-only"
		},
		"//#lint:es_:chain": {
			"cache": false,
			"outputMode": "new-only"
		},
		"lint:es_:pre": {
			"outputMode": "new-only"
		},
		"//#lint:es_:pre": {
			"outputMode": "new-only"
		},
		"lint:es_": {
			"dependsOn": ["//#lint:es_", "//#lint:es_"],
			"outputMode": "new-only"
		},
		"//#lint:es_": {
			"dependsOn": ["//#lint:es_:chain", "//#lint:es_:pre"],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:tsc_": {
			"outputs": [],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#lint:tsc_": {
			"outputs": [],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"lint:svelte_": {
			"outputs": [],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#lint:svelte_": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:style_": {
			"outputs": [],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#lint:style_": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:md_": {
			"outputs": [],
			"dependsOn": [],
			"outputMode": "new-only"
		},
		"//#lint:md_": {
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"lint:format_": {
			"env": [],
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"//#lint:format_": {
			"env": [],
			"dependsOn": [],
			"outputs": [],
			"outputMode": "new-only"
		},
		"typedoc_": {
			"dependsOn": [],
			"outputs": ["typedoc/**"],
			"outputMode": "new-only"
		},
		"//#typedoc_": {
			"dependsOn": [],
			"outputs": ["typedoc/**"],
			"outputMode": "new-only"
		},
		"test_": {
			"env": ["VITEST"],
			"dependsOn": ["^build_"],
			"outputs": ["coverage"],
			"outputMode": "new-only"
		},
		"start": {
			"cache": false
		},
		"dev": {
			"cache": false
		}
	}
}
