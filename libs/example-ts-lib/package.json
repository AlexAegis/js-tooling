{
	"name": "@alexaegis/example-ts-lib",
	"description": "Example package to be used in an app",
	"version": "0.11.1",
	"license": "MIT",
	"private": true,
	"archetype": {
		"platform": "node",
		"language": "ts",
		"kind": "lib"
	},
	"keywords": [
		"eslint",
		"managed-by-autotool"
	],
	"type": "module",
	"config": {
		"engine-strict": true
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"lint:depcheck": "turbo run lint:depcheck_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/example-ts-lib",
		"lint:depcheck_": "depcheck",
		"lint:es": "turbo run lint:es_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/example-ts-lib",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/example-ts-lib",
		"lint:format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check .",
		"lint:md": "turbo run lint:md_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/example-ts-lib",
		"lint:md_": "remark --frail --no-stdout --silently-ignore .",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/example-ts-lib",
		"lint:tsc_": "tsc --noEmit",
		"test": "turbo run test_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/example-ts-lib",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage",
		"all": "BUILD_REASON='publish' turbo run all_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/example-ts-lib",
		"build": "turbo run build-lib_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/example-ts-lib",
		"build-lib_": "vite build",
		"format": "turbo run format_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/example-ts-lib",
		"format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write ."
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"default": "./dist/index.js"
		},
		"./readme": "./readme.md"
	},
	"devDependencies": {
		"@alexaegis/eslint-config-vitest": "workspace:^",
		"@alexaegis/ts": "workspace:^",
		"@alexaegis/vite": "workspace:^",
		"@alexaegis/vitest": "workspace:^",
		"@types/eslint": "^8.56.10",
		"@types/node": "^20.12.12",
		"vite": "^5.2.11",
		"vitest": "^1.6.0"
	}
}
