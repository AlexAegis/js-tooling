{
	"name": "svelte",
	"version": "0.0.0",
	"private": true,
	"keywords": [
		"@alexaegis/setup-eslint",
		"@alexaegis/setup-prettier",
		"@alexaegis/setup-remark",
		"@alexaegis/setup-ts-svelte",
		"@alexaegis/setup-vite-svelte",
		"@alexaegis/setup-vitest-web",
		"svelte"
	],
	"type": "module",
	"scripts": {
		"build": "turbo run build_ --concurrency 6 --filter svelte",
		"build_": "vite build",
		"lint:es": "turbo run lint:es_ --concurrency 6 --filter svelte",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --filter svelte",
		"lint:format_": "prettier --check .",
		"lint:md": "turbo run lint:md_ --concurrency 6 --filter svelte",
		"lint:md_": "remark --frail --no-stdout *.md docs/**/*.md",
		"lint:svelte": "svelte-check --tsconfig ./tsconfig.json",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 6 --filter svelte",
		"lint:tsc_": "tsc --noEmit",
		"format": "prettier --write .",
		"preview": "vite preview",
		"start": "vite",
		"test": "turbo run test_ --concurrency 6 --filter svelte",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage --run"
	},
	"dependencies": {
		"@alexaegis/common": "*"
	},
	"devDependencies": {
		"@alexaegis/ts": "*",
		"@alexaegis/vite": "*",
		"@alexaegis/vitest": "*",
		"@sveltejs/vite-plugin-svelte": "^2.0.3",
		"@tsconfig/svelte": "^3.0.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.4",
		"svelte-preprocess": "^5.0.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4"
	}
}
