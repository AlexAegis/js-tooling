{
	"name": "@alexaegis/svelte-example-app",
	"version": "0.2.0",
	"private": true,
	"keywords": [
		"@alexaegis/setup-eslint",
		"@alexaegis/setup-prettier",
		"@alexaegis/setup-remark",
		"@alexaegis/setup-stylelint",
		"@alexaegis/setup-ts-svelte",
		"@alexaegis/setup-vite-svelte",
		"@alexaegis/setup-vitest-web",
		"@alexaegis/setup-svelte",
		"svelte"
	],
	"type": "module",
	"scripts": {
		"build": "turbo run build-app_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"build-app_": "vite build",
		"lint:es": "turbo run lint:es_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:format_": "prettier --check .",
		"lint:md": "turbo run lint:md_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:md_": "remark --frail --no-stdout --silently-ignore *.md docs/**/*.md",
		"lint:style": "turbo run lint:style_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:style:css": "turbo run lint:style:css_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:style:css_": "stylelint --cache true --cache-strategy content --cache-location .cache/stylelintcache-css ./**/*.css",
		"lint:style:scss": "turbo run lint:style:scss_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:style:scss_": "stylelint --customSyntax=postcss-scss --cache true --cache-strategy content --cache-location .cache/stylelintcache-scss ./**/*.scss",
		"lint:svelte": "turbo run lint:svelte_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:svelte_": "svelte-check --tsconfig ./tsconfig.json",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:tsc_": "tsc --noEmit",
		"dev": "concurrently npm:watch-deps npm:start",
		"format": "prettier --write .",
		"preview": "vite preview",
		"start": "vite",
		"test": "turbo run test_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage",
		"watch-deps": "nodemon --config ../../node_modules/@alexaegis/setup-vite/static/nodemon.json --watch ./node_modules/**/src/**/* --ext ts,tsx,mts,cts,svelte,js,jsx,mjs,cjs,json --ignore dist --exec 'turbo run build-lib_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app'"
	},
	"dependencies": {
		"@alexaegis/example": "^0.2.0"
	},
	"devDependencies": {
		"@alexaegis/eslint-config-vitest": "^0.2.0",
		"@alexaegis/stylelint-config": "^0.2.0",
		"@alexaegis/ts": "^0.2.0",
		"@alexaegis/vite": "^0.2.0",
		"@alexaegis/vitest": "^0.2.0",
		"@tsconfig/svelte": "^4.0.1",
		"@vitest/coverage-c8": "^0.30.1",
		"concurrently": "^8.0.1",
		"nodemon": "^2.0.22",
		"postcss-scss": "^4.0.6",
		"stylelint": "15.5.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-preprocess": "^5.0.3",
		"typescript": "^5.0.4",
		"vite": "^4.3.1",
		"vitest": "^0.30.1"
	}
}
