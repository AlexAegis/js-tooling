{
	"name": "@alexaegis/svelte-example-app",
	"version": "0.8.1",
	"archetype": {
		"platform": "web",
		"framework": "sveltekit",
		"language": "ts",
		"kind": "app"
	},
	"keywords": [
		"managed-by-autotool"
	],
	"type": "module",
	"scripts": {
		"build": "turbo run build-app_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"build-app_": "vite build",
		"build:dependencies": "TARGET_ENV='local' turbo run build-lib_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app # used by turbowatch",
		"lint:es": "turbo run lint:es_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check .",
		"lint:md": "turbo run lint:md_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:md_": "remark --frail --no-stdout --silently-ignore .",
		"lint:style": "turbo run lint:style_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:style:css": "turbo run lint:style:css_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:style:css_": "stylelint --cache true --cache-strategy content --cache-location .cache/stylelintcache-css --allow-empty-input '**/*.css'",
		"lint:style:html": "turbo run lint:style:html_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:style:html_": "stylelint --customSyntax=postcss-html --cache true --cache-strategy content --cache-location .cache/stylelintcache-html --allow-empty-input '**/*.{html,svelte,vue,astro,xml,php}'",
		"lint:style:scss": "turbo run lint:style:scss_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:style:scss_": "stylelint --customSyntax=postcss-scss --cache true --cache-strategy content --cache-location .cache/stylelintcache-scss --allow-empty-input '**/*.scss'",
		"lint:svelte": "turbo run lint:svelte_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:svelte_": "svelte-check --tsconfig ./tsconfig.json",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"lint:tsc_": "tsc --noEmit",
		"all": "BUILD_REASON='publish' turbo run all_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"dev": "turbowatch dev.js",
		"dev_": "TARGET_ENV='local' vite",
		"format": "turbo run format_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
		"kit:sync": "svelte-kit sync",
		"postinstall": "svelte-kit sync # pakk:not-distributed",
		"start": "turbo run start_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"start_": "TARGET_ENV='local' vite",
		"start:no-watch": "turbo run start_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"test": "turbo run test_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/svelte-example-app",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage"
	},
	"dependencies": {
		"@alexaegis/example-svelte-lib": "^0.8.1"
	},
	"devDependencies": {
		"@alexaegis/eslint-config-svelte": "^0.8.1",
		"@alexaegis/eslint-config-vitest": "^0.8.1",
		"@alexaegis/prettier-config": "^0.8.1",
		"@alexaegis/stylelint-config": "^0.8.1",
		"@alexaegis/svelte-config": "^0.8.1",
		"@alexaegis/ts": "^0.8.1",
		"@alexaegis/turbowatch": "workspace:^",
		"@alexaegis/vite": "^0.8.1",
		"@alexaegis/vitest": "^0.8.1",
		"@skeletonlabs/skeleton": "^2.0.0",
		"@skeletonlabs/tw-plugin": "^0.1.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/kit": "^1.24.0",
		"@types/node": "^20.5.8",
		"autoprefixer": "^10.4.15",
		"eslint-config-next": "^13.4.19",
		"eslint-config-prettier": "^9.0.0",
		"eslint-config-turbo": "^1.10.13",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-svelte": "^2.33.0",
		"eslint-plugin-unicorn": "^48.0.1",
		"postcss": "^8.4.29",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.0.3",
		"stylelint": "^15.10.3",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.1",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"turbowatch": "^2.29.4",
		"vite": "^4.4.9",
		"vitest": "^0.34.3"
	}
}
