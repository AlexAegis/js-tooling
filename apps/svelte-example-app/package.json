{
	"name": "@alexaegis/svelte-example-app",
	"version": "0.13.0",
	"archetype": {
		"platform": "web",
		"framework": "sveltekit",
		"language": "ts",
		"kind": "app"
	},
	"keywords": [
		"managed-by-autotool"
	],
	"type": "module",
	"scripts": {
		"lint:es": "turbo run lint:es_ --concurrency 16 --filter @alexaegis/svelte-example-app",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 16 --filter @alexaegis/svelte-example-app",
		"lint:format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check .",
		"lint:md": "turbo run lint:md_ --concurrency 16 --filter @alexaegis/svelte-example-app",
		"lint:md_": "remark --frail --no-stdout --silently-ignore .",
		"lint:style": "turbo run lint:style_ --concurrency 16 --filter @alexaegis/svelte-example-app",
		"lint:style:css": "turbo run lint:style:css_ --concurrency 16 --filter @alexaegis/svelte-example-app",
		"lint:style:css_": "stylelint --cache true --cache-strategy content --cache-location .cache/stylelintcache-css --allow-empty-input '**/*.css'",
		"lint:style:html": "turbo run lint:style:html_ --concurrency 16 --filter @alexaegis/svelte-example-app",
		"lint:style:html_": "stylelint --customSyntax=postcss-html --cache true --cache-strategy content --cache-location .cache/stylelintcache-html --allow-empty-input '**/*.{html,svelte,vue,astro,xml,php}'",
		"lint:style:scss": "turbo run lint:style:scss_ --concurrency 16 --filter @alexaegis/svelte-example-app",
		"lint:style:scss_": "stylelint --customSyntax=postcss-scss --cache true --cache-strategy content --cache-location .cache/stylelintcache-scss --allow-empty-input '**/*.scss'",
		"lint:svelte": "turbo run lint:svelte_ --concurrency 16 --filter @alexaegis/svelte-example-app",
		"lint:svelte_": "svelte-check --tsconfig ./tsconfig.json",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 16 --filter @alexaegis/svelte-example-app",
		"lint:tsc_": "tsc --noEmit",
		"test": "turbo run test_ --concurrency 16 --filter @alexaegis/svelte-example-app",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage",
		"all": "BUILD_REASON='publish' turbo run all_ --concurrency 16 --filter @alexaegis/svelte-example-app",
		"build": "turbo run build-app_ --concurrency 16 --filter @alexaegis/svelte-example-app",
		"build-app_": "vite build",
		"build:dependencies": "TARGET_ENV='local' turbo run build-lib_ --concurrency 16 --filter @alexaegis/svelte-example-app # used by turbowatch",
		"dev": "turbowatch dev.js",
		"dev_": "TARGET_ENV='local' vite",
		"format": "turbo run format_ --concurrency 16 --filter @alexaegis/svelte-example-app",
		"format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
		"kit:sync": "svelte-kit sync",
		"postinstall": "svelte-kit sync # pakk:not-distributed",
		"start": "turbo run start_ --concurrency 16 --filter @alexaegis/svelte-example-app",
		"start_": "TARGET_ENV='local' vite",
		"start:no-watch": "turbo run start_ --concurrency 16 --filter @alexaegis/svelte-example-app"
	},
	"dependencies": {
		"@alexaegis/example-svelte-lib": "workspace:^"
	},
	"devDependencies": {
		"@alexaegis/eslint-config-svelte": "workspace:^",
		"@alexaegis/eslint-config-vitest": "workspace:^",
		"@alexaegis/prettier-config": "workspace:^",
		"@alexaegis/stylelint-config": "workspace:^",
		"@alexaegis/svelte-config": "workspace:^",
		"@alexaegis/ts": "workspace:^",
		"@alexaegis/turbowatch": "workspace:^",
		"@alexaegis/vite": "workspace:^",
		"@alexaegis/vitest": "workspace:^",
		"@skeletonlabs/skeleton": "^3.1.2",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/adapter-vercel": "^5.7.0",
		"@sveltejs/kit": "^2.20.7",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "^4.1.4",
		"@types/node": "^22.14.1",
		"autoprefixer": "^10.4.21",
		"eslint-config-turbo": "^2.5.0",
		"eslint-plugin-svelte": "^3.5.1",
		"eslint-plugin-unicorn": "^58.0.0",
		"prettier": "^3.5.3",
		"skeleton": "^1.0.0",
		"stylelint": "^16.18.0",
		"svelte": "^5.28.1",
		"svelte-check": "^4.1.6",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.4",
		"turbowatch": "^2.29.4",
		"vite": "^6.3.2",
		"vitest": "^3.1.1"
	}
}
