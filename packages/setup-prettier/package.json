{
	"name": "@alexaegis/setup-prettier",
	"description": "My prettier config distributor",
	"version": "0.0.1",
	"license": "MIT",
	"private": false,
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/js#readme",
	"repository": "github:AlexAegis/js",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/js/issues"
	},
	"keywords": [
		"javascript",
		"js",
		"prettier",
		"ts",
		"typescript"
	],
	"type": "module",
	"config": {
		"engine-strict": false
	},
	"publishConfig": {
		"access": "public"
	},
	"directories": {},
	"scripts": {
		"build": "turbo run build_ --concurrency 6 --filter @alexaegis/setup-prettier",
		"build_": "vite build",
		"lint:es": "turbo run lint:es_ --concurrency 6 --filter @alexaegis/setup-prettier",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --filter @alexaegis/setup-prettier",
		"lint:format_": "prettier --check .",
		"lint:tsc": "tsc --noEmit",
		"format": "prettier --write .",
		"postinstall": "# local install hooks are disabled # autogenerated"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./readme": "./readme.md",
		"./static/.prettierignore": "./static/.prettierignore",
		"./static/.prettierrc": "./static/.prettierrc.cjs"
	},
	"bin": {
		"alexaegis-setup-prettier-postinstall": "./shims/postinstall.mjs",
		"prettier": "node_modules/prettier/bin-prettier.js",
		"setup-prettier": "./shims/setup-prettier.mjs"
	},
	"dependencies": {
		"@alexaegis/cli-tools": "0.0.16",
		"@alexaegis/logging": "0.0.16",
		"@alexaegis/workspace-tools": "0.0.16",
		"prettier": "^2.8.4",
		"yargs": "^17.6.2"
	},
	"devDependencies": {
		"@alexaegis/ts": "*",
		"@alexaegis/vite": "*",
		"@types/node": "^18.13.0",
		"@types/prettier": "2.7.2",
		"@types/yargs": "17.0.22",
		"typescript": "4.9.5"
	}
}
