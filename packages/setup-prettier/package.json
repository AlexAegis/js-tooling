{
	"name": "@alexaegis/setup-prettier",
	"description": "My prettier config distributor",
	"version": "0.0.1",
	"license": "MIT",
	"private": false,
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/js#readme",
	"repository": "github:AlexAegis/js",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/js/issues"
	},
	"keywords": [
		"@alexaegis/setup-eslint",
		"@alexaegis/setup-prettier",
		"@alexaegis/setup-ts-node",
		"@alexaegis/setup-vite",
		"@alexaegis/setup-vitest-node",
		"prettier"
	],
	"type": "module",
	"config": {
		"engine-strict": false
	},
	"publishConfig": {
		"access": "public"
	},
	"directories": {},
	"optionalDependencies": {
		"svelte": "^3.55.1"
	},
	"scripts": {
		"build": "turbo run build_ --concurrency 6 --filter @alexaegis/setup-prettier",
		"build_": "vite build",
		"lint:es": "turbo run lint:es_ --concurrency 6 --filter @alexaegis/setup-prettier",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --filter @alexaegis/setup-prettier",
		"lint:format_": "prettier --check .",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 6 --filter @alexaegis/setup-prettier",
		"lint:tsc_": "tsc --noEmit",
		"format": "prettier --write .",
		"postinstall": "# local install hooks are disabled # autogenerated",
		"test": "turbo run test_ --concurrency 6 --filter @alexaegis/setup-prettier",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage --run"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./.prettierignore": "./static/.prettierignore",
		"./.prettierrc": "./static/.prettierrc.cjs",
		"./readme": "./readme.md"
	},
	"bin": {
		"alexaegis-setup-prettier-postinstall": "./shims/postinstall.mjs",
		"prettier": "node_modules/prettier/bin-prettier.js",
		"setup-prettier": "./shims/setup-prettier.mjs"
	},
	"dependencies": {
		"@alexaegis/cli-tools": "0.0.20",
		"@alexaegis/logging": "0.0.20",
		"@alexaegis/workspace-tools": "0.0.20",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "2.9.0",
		"yargs": "^17.7.1"
	},
	"devDependencies": {
		"@alexaegis/ts": "*",
		"@alexaegis/vite": "*",
		"@alexaegis/vitest": "*",
		"@types/node": "^18.14.1",
		"@types/prettier": "2.7.2",
		"@types/yargs": "17.0.22",
		"typescript": "4.9.5"
	}
}
