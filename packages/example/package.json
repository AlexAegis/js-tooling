{
	"name": "@alexaegis/example",
	"description": "Example package to be used in an app",
	"version": "0.0.5",
	"license": "MIT",
	"private": true,
	"keywords": [
		"@alexaegis/setup-eslint",
		"@alexaegis/setup-prettier",
		"@alexaegis/setup-remark",
		"@alexaegis/setup-ts-node",
		"@alexaegis/setup-vite-lib",
		"@alexaegis/setup-vitest-node",
		"eslint"
	],
	"type": "module",
	"config": {
		"engine-strict": true
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "turbo run build-lib_ --concurrency 6 --filter @alexaegis/example",
		"build-lib_": "vite build",
		"lint:es": "turbo run lint:es_ --concurrency 6 --filter @alexaegis/example",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --filter @alexaegis/example",
		"lint:format_": "prettier --check .",
		"lint:md": "turbo run lint:md_ --concurrency 6 --filter @alexaegis/example",
		"lint:md_": "remark --frail --no-stdout --silently-ignore *.md docs/**/*.md",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 6 --filter @alexaegis/example",
		"lint:tsc_": "tsc --noEmit",
		"format": "prettier --write .",
		"test": "turbo run test_ --concurrency 6 --filter @alexaegis/example",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage --run"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./readme": "./readme.md"
	},
	"dependencies": {},
	"peerDependencies": {
		"typescript": "5.0.3"
	},
	"devDependencies": {
		"@alexaegis/eslint-config-vitest": "0.0.5",
		"@alexaegis/vite": "0.0.5",
		"@alexaegis/vitest": "0.0.5",
		"@types/eslint": "^8.37.0",
		"@types/node": "^18.15.11",
		"@vitest/coverage-c8": "^0.29.8",
		"vite": "4.2.1",
		"vitest": "^0.29.8"
	}
}
