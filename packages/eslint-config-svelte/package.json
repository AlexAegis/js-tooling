{
	"name": "@alexaegis/eslint-config-svelte",
	"description": "ESLint rules for svelte",
	"version": "0.11.0",
	"license": "MIT",
	"private": false,
	"archetype": {
		"platform": "node",
		"framework": "eslint",
		"language": "ts",
		"kind": "lib"
	},
	"keywords": [
		"eslint",
		"managed-by-autotool",
		"svelte"
	],
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"lint:depcheck": "turbo run lint:depcheck_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/eslint-config-svelte",
		"lint:depcheck_": "depcheck",
		"lint:es": "turbo run lint:es_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/eslint-config-svelte",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/eslint-config-svelte",
		"lint:format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check .",
		"lint:md": "turbo run lint:md_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/eslint-config-svelte",
		"lint:md_": "remark --frail --no-stdout --silently-ignore .",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/eslint-config-svelte",
		"lint:tsc_": "tsc --noEmit",
		"publint": "BUILD_REASON='publish' turbo run publint_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/eslint-config-svelte",
		"publint_": "publint dist",
		"test": "turbo run test_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/eslint-config-svelte",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage",
		"all": "BUILD_REASON='publish' turbo run all_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/eslint-config-svelte",
		"build": "turbo run build-lib_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/eslint-config-svelte",
		"build-lib_": "vite build",
		"format": "turbo run format_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/eslint-config-svelte",
		"format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write ."
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"default": "./dist/index.js"
		},
		"./ambient": {
			"types": "./src/ambient.d.ts",
			"import": "./dist/ambient.js",
			"require": "./dist/ambient.cjs",
			"default": "./dist/ambient.js"
		},
		"./eslint-plugin-svelte": {
			"types": "./src/eslint-plugin-svelte.d.ts",
			"import": "./dist/eslint-plugin-svelte.js",
			"require": "./dist/eslint-plugin-svelte.cjs",
			"default": "./dist/eslint-plugin-svelte.js"
		},
		"./readme": "./readme.md"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "8.0.0-alpha.13",
		"@typescript-eslint/parser": "8.0.0-alpha.14",
		"eslint": "^9.3.0",
		"eslint-plugin-svelte": "^2.39.0",
		"typescript": "^5.4.5",
		"typescript-eslint": "8.0.0-alpha.13"
	},
	"devDependencies": {
		"@alexaegis/eslint-config-vitest": "workspace:^0.11.0",
		"@alexaegis/ts": "workspace:^0.11.0",
		"@alexaegis/vite": "workspace:^0.11.0",
		"@alexaegis/vitest": "workspace:^0.11.0",
		"@types/eslint": "^8.56.10",
		"@types/node": "^20.12.12",
		"publint": "^0.2.8",
		"vite": "^5.2.11",
		"vitest": "^1.6.0"
	}
}
