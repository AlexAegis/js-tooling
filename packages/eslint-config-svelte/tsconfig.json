{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "@alexaegis/ts/base",
	"compilerOptions": {
		"types": ["node"],
		"rootDir": "../../",
		"outDir": "./dist/out-tsc"
	},
	"files": ["package.json"],
	"include": ["**/*.d.ts", "**/*.ts", "**/*.js", "**/*.cjs"],
	"exclude": ["node_modules", "dist", "coverage", "**/*.spec.ts"]
}
