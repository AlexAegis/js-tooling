{
	"name": "@alexaegis/setup-svelte",
	"description": "Setup svelte",
	"version": "0.2.0",
	"license": "MIT",
	"private": false,
	"keywords": [
		"@alexaegis/setup-depcheck",
		"@alexaegis/setup-eslint",
		"@alexaegis/setup-prettier",
		"@alexaegis/setup-publint",
		"@alexaegis/setup-remark",
		"@alexaegis/setup-ts-node",
		"@alexaegis/setup-vite-lib",
		"@alexaegis/setup-vitest-node"
	],
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "turbo run build-lib_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup-svelte",
		"build-lib_": "vite build",
		"lint:depcheck": "turbo run lint:depcheck_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup-svelte",
		"lint:depcheck_": "depcheck",
		"lint:es": "turbo run lint:es_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup-svelte",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup-svelte",
		"lint:format_": "prettier --check .",
		"lint:md": "turbo run lint:md_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup-svelte",
		"lint:md_": "remark --frail --no-stdout --silently-ignore *.md docs/**/*.md",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup-svelte",
		"lint:tsc_": "tsc --noEmit",
		"publint": "turbo run publint_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup-svelte",
		"publint_": "publint dist",
		"format": "prettier --write .",
		"postinstall": "# local install hooks are disabled # autogenerated",
		"test": "turbo run test_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup-svelte",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./readme": "./readme.md",
		"./svelte.config": "./static/svelte.config.txt"
	},
	"bin": {
		"alexaegis-setup-svelte-postinstall": "./shims/postinstall.js",
		"setup-svelte": "./shims/setup-svelte.js"
	},
	"dependencies": {
		"@alexaegis/cli-tools": "^0.1.7",
		"@alexaegis/coverage-tools": "^0.1.7",
		"@alexaegis/logging": "^0.1.7",
		"@alexaegis/workspace-tools": "^0.1.7",
		"@sveltejs/kit": "^1.16.3",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"svelte-preprocess": "^5.0.3"
	},
	"devDependencies": {
		"@alexaegis/eslint-config-vitest": "^0.2.0",
		"@alexaegis/ts": "^0.2.0",
		"@alexaegis/vite": "^0.2.0",
		"@alexaegis/vitest": "^0.2.0",
		"@types/node": "^20.1.4",
		"@vitest/coverage-c8": "^0.31.0",
		"publint": "^0.1.11",
		"typescript": "^5.0.4",
		"vite": "^4.3.5",
		"vitest": "^0.31.0"
	}
}
