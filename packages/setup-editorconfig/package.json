{
	"name": "@alexaegis/editorconfig",
	"description": "My editorconfig",
	"version": "0.0.1",
	"license": "MIT",
	"private": false,
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/js#readme",
	"repository": "github:AlexAegis/js",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/js/issues"
	},
	"keywords": [
		"eslint",
		"javascript",
		"js",
		"prettier",
		"repo",
		"ts",
		"tsconfig",
		"turbo",
		"typescript"
	],
	"type": "module",
	"config": {
		"engine-strict": false
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "turbo run build_ --concurrency 6 --filter @alexaegis/editorconfig",
		"build_": "vite build",
		"lint:es": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --filter @alexaegis/editorconfig",
		"lint:format_": "prettier --check .",
		"lint:tsc": "tsc --noEmit",
		"format": "prettier --write .",
		"postinstall": "# local install hooks are disabled # autogenerated"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./readme": "./readme.md",
		"./static/.editorconfig": "./static/.editorconfig"
	},
	"bin": {
		"alexaegis-editorconfig-postinstall": "./shims/postinstall.mjs",
		"setup-editorconfig": "./shims/setup-editorconfig.mjs"
	},
	"dependencies": {
		"@alexaegis/cli-tools": "0.0.15",
		"@alexaegis/logging": "0.0.15",
		"@alexaegis/vite": "*",
		"@alexaegis/workspace-tools": "0.0.15"
	},
	"devDependencies": {
		"@alexaegis/vite": "*",
		"@types/node": "^18.13.0",
		"typescript": "4.9.5"
	}
}
