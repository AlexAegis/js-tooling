{
	"name": "@alexaegis/prettier",
	"description": "My prettier config distributor",
	"version": "0.0.1",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/js#readme",
	"repository": "github:AlexAegis/js",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/js/issues"
	},
	"keywords": [
		"prettier",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"private": false,
	"config": {
		"engine-strict": false
	},
	"scripts": {
		"build": "vite build",
		"format": "prettier --write .",
		"lint:es": "eslint --max-warnings=0 .",
		"lint:format": "prettier --check ."
	},
	"sideEffects": false,
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"yargs": "^17.6.2",
		"prettier": "^2.7.1",
		"@alexaegis/tools": "file:../tools"
	},
	"devDependencies": {
		"@types/yargs": "17.0.13",
		"@alexaegis/vite": "file:../vite",
		"@types/node": "^18.11.9",
		"typescript": "4.8.4",
		"@types/prettier": "2.7.1"
	},
	"directories": {},
	"exports": {
		"./static/.prettierignore": "./static/.prettierignore",
		"./static/.prettierrc": "./static/.prettierrc.cjs",
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		}
	},
	"bin": {
		"distribute-prettier-config": "./shims/distribute-prettier-config.mjs"
	}
}
