{
	"name": "@alexaegis/setup-typedoc",
	"description": "My typedoc config",
	"version": "0.0.5",
	"license": "MIT",
	"private": false,
	"keywords": [
		"@alexaegis/setup-eslint",
		"@alexaegis/setup-prettier",
		"@alexaegis/setup-remark",
		"@alexaegis/setup-ts-node",
		"@alexaegis/setup-typedoc",
		"@alexaegis/setup-vite-lib",
		"@alexaegis/setup-vitest-node",
		"turbo"
	],
	"type": "module",
	"config": {
		"engine-strict": false
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "turbo run build-lib_ --concurrency 6 --filter @alexaegis/setup-typedoc",
		"build-lib_": "vite build",
		"lint:es": "turbo run lint:es_ --concurrency 6 --filter @alexaegis/setup-typedoc",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --filter @alexaegis/setup-typedoc",
		"lint:format_": "prettier --check .",
		"lint:md": "turbo run lint:md_ --concurrency 6 --filter @alexaegis/setup-typedoc",
		"lint:md_": "remark --frail --no-stdout --silently-ignore *.md docs/**/*.md",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 6 --filter @alexaegis/setup-typedoc",
		"lint:tsc_": "tsc --noEmit",
		"format": "prettier --write .",
		"postinstall": "# local install hooks are disabled # autogenerated",
		"test": "turbo run test_ --concurrency 6 --filter @alexaegis/setup-typedoc",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage --run",
		"typedoc_": "typedoc src/index.ts --out dist/docs"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./readme": "./readme.md",
		"./turbo": "./static/turbo.json"
	},
	"bin": {
		"alexaegis-setup-typedoc-postinstall": "./shims/postinstall.js",
		"setup-typedoc": "./shims/setup-typedoc.js"
	},
	"dependencies": {
		"@alexaegis/cli-tools": "0.0.23",
		"@alexaegis/logging": "0.0.23",
		"@alexaegis/workspace-tools": "0.0.23",
		"typedoc": "^0.23.28"
	},
	"devDependencies": {
		"@alexaegis/vite": "0.0.5",
		"@alexaegis/vitest": "0.0.5",
		"@types/node": "^18.15.11",
		"@vitest/coverage-c8": "^0.29.8",
		"typescript": "5.0.3",
		"vite": "4.2.1",
		"vitest": "^0.29.8"
	}
}
