{
	"name": "@alexaegis/vitest",
	"description": "My vitest config",
	"version": "0.2.0",
	"license": "MIT",
	"private": false,
	"keywords": [
		"@alexaegis/setup-depcheck",
		"@alexaegis/setup-eslint",
		"@alexaegis/setup-prettier",
		"@alexaegis/setup-remark",
		"@alexaegis/setup-vite-lib"
	],
	"type": "module",
	"config": {
		"engine-strict": false
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "turbo run build-lib_ --concurrency 32 --cache-dir .cache/turbo --filter @alexaegis/vitest",
		"build-lib_": "vite build",
		"lint:depcheck": "turbo run lint:depcheck_ --concurrency 32 --cache-dir .cache/turbo --filter @alexaegis/vitest",
		"lint:depcheck_": "depcheck",
		"lint:es": "turbo run lint:es_ --concurrency 32 --cache-dir .cache/turbo --filter @alexaegis/vitest",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 32 --cache-dir .cache/turbo --filter @alexaegis/vitest",
		"lint:format_": "prettier --check .",
		"lint:md": "turbo run lint:md_ --concurrency 32 --cache-dir .cache/turbo --filter @alexaegis/vitest",
		"lint:md_": "remark --frail --no-stdout --silently-ignore *.md docs/**/*.md",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 32 --cache-dir .cache/turbo --filter @alexaegis/vitest",
		"lint:tsc_": "tsc --noEmit",
		"format": "prettier --write .",
		"test": "turbo run test_ --concurrency 32 --cache-dir .cache/turbo --filter @alexaegis/vitest",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage --run"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./readme": "./readme.md",
		"./vitest-node.config": {
			"types": "./src/vitest-node.config.ts",
			"import": "./dist/vitest-node.config.js",
			"require": "./dist/vitest-node.config.cjs"
		},
		"./vitest-svelte.config": {
			"types": "./src/vitest-svelte.config.ts",
			"import": "./dist/vitest-svelte.config.js",
			"require": "./dist/vitest-svelte.config.cjs"
		},
		"./vitest-web.config": {
			"types": "./src/vitest-web.config.ts",
			"import": "./dist/vitest-web.config.js",
			"require": "./dist/vitest-web.config.cjs"
		}
	},
	"bin": {
		"vitest": "node_modules/.bin/vitest"
	},
	"dependencies": {
		"@sveltejs/vite-plugin-svelte": "^2.1.1",
		"@vitest/coverage-c8": "^0.30.1",
		"@vitest/ui": "^0.30.1",
		"jsdom": "21.1.1",
		"vitest": "^0.30.1"
	},
	"peerDependencies": {
		"vite": "^4.2.1",
		"vitest": "^0.30.1"
	},
	"devDependencies": {
		"@alexaegis/vite": "^0.2.0",
		"vite": "^4.3.3"
	}
}
