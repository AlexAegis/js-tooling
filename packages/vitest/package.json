{
	"name": "@alexaegis/vitest",
	"description": "My vitest config",
	"version": "0.0.1",
	"license": "MIT",
	"private": false,
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/js#readme",
	"repository": "github:AlexAegis/js",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/js/issues"
	},
	"keywords": [
		"@alexaegis/setup-eslint",
		"@alexaegis/setup-prettier",
		"@alexaegis/setup-ts-node",
		"@alexaegis/setup-vite",
		"@alexaegis/setup-vitest",
		"vitest"
	],
	"type": "module",
	"config": {
		"engine-strict": false
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "turbo run build_ --concurrency 6 --filter @alexaegis/vitest",
		"build_": "vite build",
		"lint:es": "turbo run lint:es_ --concurrency 6 --filter @alexaegis/vitest",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --filter @alexaegis/vitest",
		"lint:format_": "prettier --check .",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 6 --filter @alexaegis/vitest",
		"lint:tsc_": "tsc --noEmit",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./readme": "./readme.md"
	},
	"bin": {
		"vitest": "node_modules/vitest/vitest.mjs"
	},
	"dependencies": {
		"@vitest/coverage-c8": "^0.28.5",
		"@vitest/ui": "^0.28.5",
		"vitest": "^0.28.5"
	},
	"peerDependencies": {
		"vite": "^3.2.4",
		"vitest": "^0.27.1"
	},
	"devDependencies": {
		"@alexaegis/vite": "*",
		"tslib": "^2.5.0"
	}
}
