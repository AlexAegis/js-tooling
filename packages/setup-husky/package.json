{
	"name": "@alexaegis/setup-husky",
	"description": "Husky, lint-staged and commitlint stuff",
	"version": "0.2.0",
	"license": "MIT",
	"private": false,
	"keywords": [
		"@alexaegis/setup-depcheck",
		"@alexaegis/setup-eslint",
		"@alexaegis/setup-prettier",
		"@alexaegis/setup-publint",
		"@alexaegis/setup-remark",
		"@alexaegis/setup-ts-node",
		"@alexaegis/setup-vite-lib",
		"@alexaegis/setup-vitest-node",
		"husky"
	],
	"type": "module",
	"config": {
		"engine-strict": false
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "turbo run build-lib_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/setup-husky",
		"build-lib_": "vite build",
		"lint:depcheck": "turbo run lint:depcheck_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/setup-husky",
		"lint:depcheck_": "depcheck",
		"lint:es": "turbo run lint:es_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/setup-husky",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/setup-husky",
		"lint:format_": "prettier --check .",
		"lint:md": "turbo run lint:md_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/setup-husky",
		"lint:md_": "remark --frail --no-stdout --silently-ignore *.md docs/**/*.md",
		"lint:publint": "turbo run lint:publint_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/setup-husky",
		"lint:publint_": "publint",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/setup-husky",
		"lint:tsc_": "tsc --noEmit",
		"format": "prettier --write .",
		"postinstall": "# local install hooks are disabled # autogenerated",
		"test": "turbo run test_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/setup-husky",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./commit-msg": "./static/.husky/commit-msg",
		"./commitlint.config": "./static/commitlint.config.cjs",
		"./lint-staged.config": "./static/lint-staged.config.mjs",
		"./pre-commit": "./static/.husky/pre-commit",
		"./readme": "./readme.md"
	},
	"bin": {
		"alexaegis-setup-husky-postinstall": "./shims/postinstall.js",
		"setup-husky": "./shims/setup-husky.js"
	},
	"dependencies": {
		"@alexaegis/cli-tools": "^0.1.4",
		"@alexaegis/lint-staged-config": "^0.2.0",
		"@alexaegis/logging": "^0.1.4",
		"@alexaegis/workspace-tools": "^0.1.4",
		"@commitlint/config-conventional": "^17.6.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.1"
	},
	"peerDependencies": {
		"@alexaegis/setup-commitlint": "^0.2.0"
	},
	"devDependencies": {
		"@alexaegis/eslint-config-vitest": "^0.2.0",
		"@alexaegis/ts": "^0.2.0",
		"@alexaegis/vite": "^0.2.0",
		"@alexaegis/vitest": "^0.2.0",
		"@types/node": "^18.16.0",
		"@vitest/coverage-c8": "^0.30.1",
		"publint": "^0.1.11",
		"typescript": "^5.0.4",
		"vite": "^4.3.1",
		"vitest": "^0.30.1"
	}
}
