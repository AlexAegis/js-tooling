{
	"name": "@alexaegis/tools",
	"description": "Stuff",
	"version": "0.0.1",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/js#readme",
	"repository": "github:AlexAegis/js",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/js/issues"
	},
	"keywords": [
		"link",
		"symlink",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"private": false,
	"config": {
		"engine-strict": true
	},
	"sideEffects": false,
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"bin": "ts-node-esm ./src/bin/index.ts",
		"build": "vite build",
		"test": "vitest --passWithNoTests --coverage --run",
		"format": "prettier --write .",
		"lint:es": "eslint --max-warnings=0 .",
		"lint:format": "prettier --check .",
		"postinstall": "node --loader ts-node/esm src/bin/postinstall.ts # autogenerated"
	},
	"dependencies": {
		"globby": "^13.1.2",
		"kolorist": "^1.6.0",
		"js-yaml": "^4.1.0"
	},
	"optionalDependencies": {
		"yargs": "^17.6.2"
	},
	"devDependencies": {
		"typescript": "^4.8.4",
		"@types/yargs": "^17.0.13",
		"@types/js-yaml": "^4.0.5",
		"@alexaegis/tsconfig": "file:../tsconfig",
		"@alexaegis/vite": "file:../vite",
		"@alexaegis/vitest": "file:../vitest",
		"@alexaegis/eslint-config-core": "file:../eslint-config-core",
		"@schemastore/package": "^0.0.6",
		"@types/node": "^18.11.9",
		"ts-node": "^10.9.1",
		"sort-package-json": "2.1.0"
	},
	"directories": {},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		}
	},
	"bin": {
		"distribute": "./shims/distribute.mjs",
		"alexaegis-tools-postinstall": "./shims/postinstall.mjs"
	}
}
