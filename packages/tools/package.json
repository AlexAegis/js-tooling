{
	"name": "@alexaegis/tools",
	"description": "Stuff",
	"version": "0.0.1",
	"license": "MIT",
	"private": false,
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/js#readme",
	"repository": "github:AlexAegis/js",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/js/issues"
	},
	"keywords": [
		"javascript",
		"js",
		"link",
		"symlink",
		"ts",
		"typescript"
	],
	"type": "module",
	"sideEffects": false,
	"config": {
		"engine-strict": true
	},
	"publishConfig": {
		"access": "public"
	},
	"optionalDependencies": {
		"yargs": "^17.6.2"
	},
	"scripts": {
		"build": "vite build",
		"bin": "ts-node-esm ./src/bin/index.ts",
		"format": "prettier --write .",
		"lint:es": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "prettier --check .",
		"lint:tsc": "tsc --noEmit",
		"postinstall": "# local install hooks are disabled # autogenerated",
		"test": "vitest --passWithNoTests --coverage --run"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./readme": "./readme.md"
	},
	"bin": {
		"alexaegis-tools-postinstall": "./shims/postinstall.mjs",
		"distribute": "./shims/distribute.mjs"
	},
	"dependencies": {
		"globby": "^13.1.3",
		"js-yaml": "^4.1.0",
		"kolorist": "^1.6.0"
	},
	"devDependencies": {
		"@alexaegis/eslint-config-core": "*",
		"@alexaegis/ts": "*",
		"@alexaegis/vite": "*",
		"@alexaegis/vitest": "*",
		"@schemastore/package": "^0.0.6",
		"@types/js-yaml": "^4.0.5",
		"@types/node": "^18.11.18",
		"@types/yargs": "^17.0.19",
		"sort-package-json": "2.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.4",
		"vitest": "^0.27.1"
	}
}
