{
	"name": "@alexaegis/setup-vitest",
	"description": "Setup vitest",
	"version": "0.0.1",
	"license": "MIT",
	"private": false,
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "turbo run build_ --concurrency 6 --filter @alexaegis/setup-vitest",
		"build_": "vite build",
		"lint:es": "turbo run lint:es_ --concurrency 6 --filter @alexaegis/setup-vitest",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --filter @alexaegis/setup-vitest",
		"lint:format_": "prettier --check .",
		"lint:tsc": "tsc --noEmit",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./readme": "./readme.md"
	},
	"dependencies": {
		"@alexaegis/cli-tools": "0.0.15",
		"@alexaegis/logging": "0.0.15",
		"@alexaegis/workspace-tools": "0.0.15"
	},
	"devDependencies": {
		"@alexaegis/vite": "*",
		"@types/node": "^18.13.0",
		"typescript": "4.9.5"
	}
}
