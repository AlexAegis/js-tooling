{
	"name": "@alexaegis/setup-ts",
	"description": "Exports custom updaters for standard-version",
	"version": "0.0.1",
	"license": "MIT",
	"private": false,
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/js#readme",
	"repository": "github:AlexAegis/js",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/js/issues"
	},
	"keywords": [
		"@alexaegis/setup-eslint",
		"@alexaegis/setup-prettier",
		"@alexaegis/setup-ts-node",
		"@alexaegis/setup-vite",
		"@alexaegis/setup-vitest-node",
		"tsconfig",
		"typescript"
	],
	"type": "module",
	"config": {
		"engine-strict": false
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "turbo run build_ --concurrency 6 --filter @alexaegis/setup-ts",
		"build_": "vite build",
		"lint:es": "turbo run lint:es_ --concurrency 6 --filter @alexaegis/setup-ts",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --filter @alexaegis/setup-ts",
		"lint:format_": "prettier --check .",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 6 --filter @alexaegis/setup-ts",
		"lint:tsc_": "tsc --noEmit",
		"format": "prettier --write .",
		"postinstall": "# local install hooks are disabled # autogenerated",
		"test": "turbo run test_ --concurrency 6 --filter @alexaegis/setup-ts",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage --run"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./package-composing-tsconfig": "./static/package-composing-tsconfig.json",
		"./package-source-tsconfig": "./static/package-source-tsconfig.json",
		"./package-spec-vitest-tsconfig": "./static/package-spec-vitest-tsconfig.json",
		"./readme": "./readme.md",
		"./workspace-tsconfig": "./static/workspace-tsconfig.json"
	},
	"bin": {
		"alexaegis-setup-ts-postinstall": "./shims/postinstall.mjs",
		"setup-ts": "./shims/setup-ts.mjs"
	},
	"dependencies": {
		"@alexaegis/cli-tools": "0.0.19",
		"@alexaegis/logging": "0.0.19",
		"@alexaegis/workspace-tools": "0.0.19"
	},
	"devDependencies": {
		"@alexaegis/vite": "*",
		"@alexaegis/vitest": "*",
		"@types/node": "^18.14.1",
		"typescript": "4.9.5"
	}
}
