{
	"name": "@alexaegis/setup-ts",
	"description": "Exports custom updaters for standard-version",
	"version": "0.0.1",
	"license": "MIT",
	"private": false,
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/js#readme",
	"repository": "github:AlexAegis/js",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/js/issues"
	},
	"keywords": [
		"javascript",
		"js",
		"ts",
		"turbo",
		"typescript"
	],
	"type": "module",
	"config": {
		"engine-strict": false
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "turbo run build_ --concurrency 6 --filter @alexaegis/setup-ts",
		"build_": "vite build",
		"lint:es": "turbo run lint:es_ --concurrency 6 --filter @alexaegis/setup-ts",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --filter @alexaegis/setup-ts",
		"lint:format_": "prettier --check .",
		"lint:tsc": "tsc --noEmit",
		"format": "prettier --write .",
		"postinstall": "# local install hooks are disabled # autogenerated"
	},
	"exports": {
		"./package-tsconfig": "./static/package-tsconfig.json",
		"./package-tsconfig.spec": "./static/package-tsconfig.spec.json",
		"./readme": "./readme.md",
		"./workspace-tsconfig": "./static/workspace-tsconfig.json"
	},
	"bin": {
		"alexaegis-setup-ts-postinstall": "./shims/postinstall.mjs",
		"setup-ts": "./shims/setup-ts.mjs"
	},
	"dependencies": {
		"@alexaegis/cli-tools": "0.0.16",
		"@alexaegis/logging": "0.0.16",
		"@alexaegis/workspace-tools": "0.0.16"
	},
	"devDependencies": {
		"@alexaegis/vite": "*",
		"@types/node": "^18.13.0",
		"typescript": "4.9.5"
	}
}
