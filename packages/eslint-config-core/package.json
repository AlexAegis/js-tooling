{
	"name": "@alexaegis/eslint-config-core",
	"description": "ESLint setup",
	"version": "0.0.1",
	"license": "MIT",
	"private": false,
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/js#readme",
	"repository": "github:AlexAegis/js",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/js/issues"
	},
	"keywords": [
		"@alexaegis/setup-eslint",
		"@alexaegis/setup-prettier",
		"@alexaegis/setup-ts-node",
		"@alexaegis/setup-vite",
		"@alexaegis/setup-vitest-node",
		"eslint"
	],
	"type": "module",
	"config": {
		"engine-strict": true
	},
	"publishConfig": {
		"access": "public"
	},
	"optionalDependencies": {
		"svelte": "^3.55.1"
	},
	"scripts": {
		"build": "turbo run build_ --concurrency 6 --filter @alexaegis/eslint-config-core",
		"build_": "vite build",
		"lint:es": "turbo run lint:es_ --concurrency 6 --filter @alexaegis/eslint-config-core",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --filter @alexaegis/eslint-config-core",
		"lint:format_": "prettier --check .",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 6 --filter @alexaegis/eslint-config-core",
		"lint:tsc_": "tsc --noEmit",
		"format": "prettier --write .",
		"test": "turbo run test_ --concurrency 6 --filter @alexaegis/eslint-config-core",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage --run"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./readme": "./readme.md"
	},
	"bin": {
		"eslint": "node_modules/eslint/bin/eslint.js"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"eslint": "^8.34.0",
		"eslint-config-next": "^13.2.1",
		"eslint-config-prettier": "^8.6.0",
		"eslint-config-turbo": "^0.0.8",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-svelte3": "^4.0.0",
		"eslint-plugin-unicorn": "^45.0.2",
		"typescript": "^4.9.5"
	},
	"peerDependencies": {
		"typescript": "^4.9.5"
	},
	"devDependencies": {
		"@alexaegis/vite": "*",
		"@alexaegis/vitest": "*",
		"@types/eslint": "^8.21.1",
		"@types/node": "^18.14.1"
	}
}
