{
	"name": "@alexaegis/eslint-config-core",
	"description": "ESLint setup",
	"version": "0.0.1",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/js#readme",
	"repository": "github:AlexAegis/js",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/js/issues"
	},
	"keywords": [
		"link",
		"symlink",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"private": false,
	"config": {
		"engine-strict": true
	},
	"sideEffects": false,
	"type": "module",
	"main": "./index.cjs",
	"exports": {
		".": "./index.cjs"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"format": "prettier --write .",
		"lint:es": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:tsc": "tsc --noEmit",
		"lint:format": "prettier --check ."
	},
	"dependencies": {
		"eslint": "^8.33.0",
		"eslint-config-next": "^13.1.6",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-config-turbo": "^0.0.7",
		"eslint-plugin-unicorn": "^45.0.2",
		"@typescript-eslint/parser": "^5.49.0",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"typescript": "^4.9.5"
	},
	"devDependencies": {
		"@types/node": "^18.11.18"
	},
	"bin": {
		"eslint": "node_modules/eslint/bin/eslint.js"
	}
}
