{
	"name": "@alexaegis/vite",
	"description": "My vite config",
	"version": "0.0.1",
	"license": "MIT",
	"private": false,
	"keywords": [
		"@alexaegis/setup-eslint",
		"@alexaegis/setup-prettier",
		"@alexaegis/setup-remark",
		"@alexaegis/setup-ts-node",
		"vite"
	],
	"type": "module",
	"config": {
		"engine-strict": false
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build-lib": "turbo run build-lib_ --concurrency 6 --filter @alexaegis/vite",
		"build-lib_": "vite build",
		"lint:es": "turbo run lint:es_ --concurrency 6 --filter @alexaegis/vite",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --filter @alexaegis/vite",
		"lint:format_": "prettier --check .",
		"lint:md": "turbo run lint:md_ --concurrency 6 --filter @alexaegis/vite",
		"lint:md_": "remark --frail --no-stdout *.md docs/**/*.md",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 6 --filter @alexaegis/vite",
		"lint:tsc_": "tsc --noEmit",
		"format": "prettier --write .",
		"prebuild": "turbo run prebuild_ --concurrency 6 --filter @alexaegis/vite",
		"prebuild_": "vite build",
		"test": "turbo run test_ --concurrency 6 --filter  @alexaegis/vite && merge-workspace-lcov-reports && lcov-viewer lcov -o ./coverage ./coverage/lcov.info",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage --run"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./app-svelte-vite.config": {
			"types": "./src/app-svelte-vite.config.ts",
			"import": "./dist/app-svelte-vite.config.js",
			"require": "./dist/app-svelte-vite.config.cjs"
		},
		"./base-vite.config": {
			"types": "./src/base-vite.config.ts",
			"import": "./dist/base-vite.config.js",
			"require": "./dist/base-vite.config.cjs"
		},
		"./example": "./static/example.json",
		"./library-vite.config": {
			"types": "./src/library-vite.config.ts",
			"import": "./dist/library-vite.config.js",
			"require": "./dist/library-vite.config.cjs"
		},
		"./readme": "./readme.md"
	},
	"bin": {
		"concurrently": "node_modules/concurrently/index.js",
		"nodemon": "node_modules/nodemon/bin/nodemon.js",
		"vite": "node_modules/vite/bin/vite.js"
	},
	"dependencies": {
		"@sveltejs/vite-plugin-svelte": "^2.0.3",
		"concurrently": "^7.6.0",
		"nodemon": "^2.0.21",
		"svelte": "^3.55.1",
		"vite": "4.1.4",
		"vite-plugin-autolib": "^0.0.10",
		"vite-plugin-dts": "^2.0.2",
		"vite-plugin-static-copy": "0.13.1"
	},
	"optionalDependencies": {
		"prettier": "^2.8.4"
	},
	"devDependencies": {
		"@schemastore/package": "^0.0.8",
		"@types/fs-extra": "^11.0.1",
		"@types/node": "^18.14.6",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5",
		"vitest": "^0.29.2"
	}
}
