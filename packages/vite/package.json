{
	"name": "@alexaegis/vite",
	"description": "My vite config",
	"version": "0.8.1",
	"license": "MIT",
	"private": false,
	"archetype": {
		"platform": "node",
		"framework": "vite",
		"language": "ts",
		"kind": "lib"
	},
	"keywords": [
		"managed-by-autotool",
		"vite"
	],
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "turbo run build-lib_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"build-lib": "turbo run build-lib_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"build-lib_": "vite build",
		"lint:depcheck": "turbo run lint:depcheck_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"lint:depcheck_": "depcheck",
		"lint:es": "turbo run lint:es_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"lint:format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check .",
		"lint:md": "turbo run lint:md_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"lint:md_": "remark --frail --no-stdout --silently-ignore .",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"lint:tsc_": "tsc --noEmit",
		"publint": "BUILD_REASON='publish' turbo run publint_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"publint_": "publint dist",
		"all": "BUILD_REASON='publish' turbo run all_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"format": "turbo run format_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
		"test": "turbo run test_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"default": "./dist/index.js"
		},
		"./base-vite.config": {
			"types": "./src/base-vite.config.ts",
			"import": "./dist/base-vite.config.js",
			"require": "./dist/base-vite.config.cjs",
			"default": "./dist/base-vite.config.js"
		},
		"./example": "./static/example.json",
		"./library-vite.config": {
			"types": "./src/library-vite.config.ts",
			"import": "./dist/library-vite.config.js",
			"require": "./dist/library-vite.config.cjs",
			"default": "./dist/library-vite.config.js"
		},
		"./readme": "./readme.md"
	},
	"dependencies": {
		"@alexaegis/logging": "^0.7.0",
		"vite": "^4.4.9",
		"vite-plugin-pakk": "^0.6.0"
	},
	"devDependencies": {
		"@types/node": "^20.5.7",
		"publint": "^0.2.2",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2",
		"vitest": "^0.34.3"
	}
}
