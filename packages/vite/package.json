{
	"name": "@alexaegis/vite",
	"description": "My vite config",
	"version": "0.2.0",
	"license": "MIT",
	"private": false,
	"keywords": [
		"@alexaegis/setup-depcheck",
		"@alexaegis/setup-eslint",
		"@alexaegis/setup-prettier",
		"@alexaegis/setup-remark",
		"vite"
	],
	"type": "module",
	"config": {
		"engine-strict": false
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build-lib": "turbo run build-lib_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"build-lib_": "vite build",
		"lint:depcheck": "turbo run lint:depcheck_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"lint:depcheck_": "depcheck",
		"lint:es": "turbo run lint:es_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"lint:format_": "prettier --check .",
		"lint:md": "turbo run lint:md_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"lint:md_": "remark --frail --no-stdout --silently-ignore *.md docs/**/*.md",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"lint:tsc_": "tsc --noEmit",
		"format": "prettier --write .",
		"prebuild": "turbo run prebuild_ --concurrency 6 --cache-dir .cache/turbo --filter @alexaegis/vite",
		"prebuild_": "vite build",
		"test": "turbo run test_ --concurrency 6 --cache-dir .cache/turbo --filter  @alexaegis/vite && merge-workspace-lcov-reports && lcov-viewer lcov -o ./coverage ./coverage/lcov.info",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage --run"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./app-svelte-vite.config": {
			"types": "./src/app-svelte-vite.config.ts",
			"import": "./dist/app-svelte-vite.config.js",
			"require": "./dist/app-svelte-vite.config.cjs"
		},
		"./base-vite.config": {
			"types": "./src/base-vite.config.ts",
			"import": "./dist/base-vite.config.js",
			"require": "./dist/base-vite.config.cjs"
		},
		"./example": "./static/example.json",
		"./library-vite.config": {
			"types": "./src/library-vite.config.ts",
			"import": "./dist/library-vite.config.js",
			"require": "./dist/library-vite.config.cjs"
		},
		"./readme": "./readme.md"
	},
	"dependencies": {
		"@sveltejs/vite-plugin-svelte": "^2.1.0",
		"concurrently": "^8.0.1",
		"nodemon": "^2.0.22",
		"svelte": "^3.58.0",
		"vite": "^4.3.1",
		"vite-plugin-autolib": "^0.1.0",
		"vite-plugin-dts": "^2.3.0"
	},
	"optionalDependencies": {
		"prettier": "^2.8.7"
	},
	"devDependencies": {
		"@types/node": "^18.15.13",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4",
		"vitest": "^0.30.1"
	}
}
