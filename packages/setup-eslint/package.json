{
	"name": "@alexaegis/setup-eslint",
	"description": "Setup eslint",
	"version": "0.0.1",
	"license": "MIT",
	"private": false,
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "turbo run build_ --concurrency 6 --filter @alexaegis/setup-eslint",
		"build_": "vite build",
		"lint:es": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 6 --filter @alexaegis/setup-eslint",
		"lint:format_": "prettier --check .",
		"lint:tsc": "tsc --noEmit",
		"format": "prettier --write .",
		"postinstall": "# local install hooks are disabled # autogenerated"
	},
	"exports": {
		"./readme": "./readme.md"
	},
	"bin": {
		"alexaegis-setup-eslint-postinstall": "./shims/postinstall.mjs",
		"distribute-eslint-config": "./shims/distribute-eslint-config.mjs"
	},
	"dependencies": {
		"@alexaegis/cli-tools": "0.0.12",
		"@alexaegis/logging": "0.0.12",
		"@alexaegis/workspace-tools": "0.0.12"
	},
	"devDependencies": {
		"@alexaegis/setup-prettier": "*",
		"@alexaegis/setup-ts": "*",
		"@alexaegis/setup-vite": "*",
		"@alexaegis/vite": "*",
		"@types/node": "^18.11.19",
		"typescript": "4.9.5"
	}
}
