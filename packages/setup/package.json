{
	"name": "@alexaegis/setup",
	"description": "Setup orchestrator",
	"version": "0.2.0",
	"license": "MIT",
	"private": false,
	"keywords": [
		"@alexaegis/setup-depcheck",
		"@alexaegis/setup-eslint",
		"@alexaegis/setup-prettier",
		"@alexaegis/setup-publint",
		"@alexaegis/setup-remark",
		"@alexaegis/setup-ts-node",
		"@alexaegis/setup-vite-lib",
		"@alexaegis/setup-vitest-node",
		"git"
	],
	"archetype": {},
	"type": "module",
	"config": {
		"engine-strict": false
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "turbo run build-lib_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup",
		"build-lib_": "vite build",
		"lint:depcheck": "turbo run lint:depcheck_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup",
		"lint:depcheck_": "depcheck",
		"lint:es": "turbo run lint:es_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup",
		"lint:format_": "prettier --check .",
		"lint:md": "turbo run lint:md_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup",
		"lint:md_": "remark --frail --no-stdout --silently-ignore *.md docs/**/*.md",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup",
		"lint:tsc_": "tsc --noEmit",
		"publint": "turbo run publint_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup",
		"publint_": "publint dist",
		"format": "prettier --write .",
		"postinstall": "# local install hooks are disabled # autogenerated",
		"test": "turbo run test_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/setup",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./readme": "./readme.md"
	},
	"bin": {
		"alexaegis-setup-postinstall": "./shims/postinstall.js",
		"setup-everything": "./shims/setup-everything.js"
	},
	"dependencies": {
		"@alexaegis/cli-tools": "^0.1.5",
		"@alexaegis/common": "^0.1.5",
		"@alexaegis/fs": "^0.1.5",
		"@alexaegis/logging": "^0.1.5",
		"@alexaegis/object-match": "workspace:^",
		"@alexaegis/setup-plugin": "workspace:^",
		"@alexaegis/setup-ts": "workspace:^",
		"@alexaegis/workspace-tools": "^0.1.5",
		"globby": "^13.1.4",
		"minimatch": "^9.0.0"
	},
	"devDependencies": {
		"@alexaegis/eslint-config-vitest": "workspace:^",
		"@alexaegis/ts": "workspace:^",
		"@alexaegis/vite": "workspace:^",
		"@alexaegis/vitest": "workspace:^",
		"@types/node": "^18.16.3",
		"@vitest/coverage-c8": "^0.30.1",
		"publint": "^0.1.11",
		"typescript": "^5.0.4",
		"vite": "^4.3.3",
		"vitest": "^0.30.1"
	}
}
